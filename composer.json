{
    "name": "Vatsimphp",
    "description": "PHP library for VATSIM.net",
    "keywords": ["vatsim", "virtual air traffic", "flight simulator", "online flight simulation"],
    "homepage": "http://github.com/skymeyer/Vatsimphp",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Jelle Vink",
            "email": "jelle.vink@gmail.com",
            "homepage": "http://skymeyer.be"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "monolog/monolog": "~1.5"
    },
    "suggest": {
        "monolog/monolog": "Solid PSR-3 logging library"
    },
    "autoload": {
        "psr-0": {
            "Vatsimphp": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
